pragma solidity ^0.8.20;







///// OnChain FlyingCows
///// Developed by CaptainMasala.eth - @CaptainMasala
contract OnChainFlyingCows is ERC721A, Ownable, MooData, ReentrancyGuard  {  

    address public deployer;
    bytes32 private lastBlockHash;
    uint256 private lastBlockNumber;

    /// Mint Settings
    uint     public maxSupply    = 1333;
    uint     public mintPrice    = 0.00777 ether;
    uint     public maxFree      = 400;
    uint     public freeCount    = 0;
    bool     public mintEnabled  = false;

    /// Mint Rules
    uint     public maxMintPerTrans = 33;
    uint     public maxMintPerWallet = 333;

    /// Whitelist Settings
    mapping(address => uint) public mintAmount;
    mapping(address => bool) public whiteListed;


    constructor() ERC721A("OnChain FlyingCows", "MOO") Ownable(msg.sender){
        deployer = msg.sender;
        lastBlockHash = blockhash(block.number - 1);
        lastBlockNumber = block.number;
        _reserveMoo(); /// Initial profile pic and Captain gets to pick their Moo!! 
    }

    function _startTokenId() internal pure override returns (uint256) {
        return 1;
    }

    function mint(uint256 quantity) external payable {
        uint256 cost = mintPrice;
        require(mintEnabled, "Mint not ready yet");
        require(msg.value == quantity * cost, "Please send the exact ETH amount");
        require(quantity <= maxMintPerTrans, "Exceeds max mint per transaction");

        // Check if the mint quantity exceeds the per wallet limit
        uint256 totalMintedByWallet = mintAmount[msg.sender] + quantity;
        require(totalMintedByWallet <= maxMintPerWallet, "Exceeds max mint per wallet");
        mintAmount[msg.sender] = totalMintedByWallet;

        // Start minting
        _internalMint(quantity);
    }

    function free_mint() external {
        require(whiteListed[msg.sender] == true, "Not on whitelist");
        require(mintEnabled, "Mint not ready yet");
        require(freeCount + 1 <= maxFree, "No more free Moos!");

        whiteListed[msg.sender] = false;
        freeCount = freeCount + 1;

        // Start minting
        _internalMint(1);

    }

    function _internalMint(uint256 quantity) internal  {

        require(_totalMinted() + quantity <= maxSupply, "Sold Out!");
        require(msg.sender == tx.origin, "The minter is another contract");

        // What token do we start minting with?
        uint startTokenID = _startTokenId() + _totalMinted();
        uint mintUntilTokenID =  quantity + startTokenID;

        for(uint256 tokenId = startTokenID; tokenId < mintUntilTokenID; tokenId++) {

            /// got get our random traits
            uint[9] memory randomSeeds = _randomSeed(lastBlockHash,tokenId);

            /// set this new moo traits!
            _setMooTraits(tokenId, randomSeeds);

        }
        lastBlockHash = blockhash(block.number - 1);
        lastBlockNumber = block.number;
        _safeMint(msg.sender, quantity);

    }

    function _randomSeed(bytes32 _lastBlockHash, uint256 _tokenId) internal pure returns (uint[9] memory _randomSeeds) {
        // Initial seed
        _randomSeeds[0] = uint256(keccak256(abi.encodePacked(_lastBlockHash, _tokenId))) % 101;

        // Generate subsequent seeds
        for (uint i = 1; i < 9; i++) {
            _randomSeeds[i] = uint256(keccak256(abi.encodePacked(_randomSeeds[i - 1], _tokenId))) % 101;
        }

        return _randomSeeds;
    }

    function _setMooTraits(uint _tokenID, uint[9] memory _randomSeeds) internal {
        // Randomly select traits
        uint randBody   = _pickTraitByProbability(_randomSeeds[0], body_data, body_probability);
        uint randSpot  = _pickTraitByProbability(_randomSeeds[1], spot_data, spot_probability);
        uint randMouth   = _pickTraitByProbability(_randomSeeds[2], mouth_data, mouth_probability);
        uint randEaring = _pickTraitByProbability(_randomSeeds[3], earring_data, earring_probability);
        uint randCig = _pickTraitByProbability(_randomSeeds[4], cig_data, cig_probability);
        uint randHorn   = _pickTraitByProbability(_randomSeeds[5], horn_data, horn_probability);
        uint randFlying   = _pickTraitByProbability(_randomSeeds[6], flying_data, flying_probability);
        uint randEye   = _pickTraitByProbability(_randomSeeds[7], eye_data, eye_probability);
        uint randWeather   = _pickTraitByProbability(_randomSeeds[8], weather_data, weather_probability);


        TraitStruct memory newTraits = TraitStruct({
            body: randBody,
            spot: randSpot,
            mouth: randMouth,
            earring: randEaring,
            cig: randCig,
            horn: randHorn,
            flying: randFlying,
            eye: randEye,
            weather: randWeather
        });

        // Assign the generated traits to the token
        tokenTraits[_tokenID] = newTraits;

    }

    function _pickTraitByProbability(uint seed, bytes[] memory traitArray, uint[] memory traitProbability) internal pure returns (uint) {
        require(traitArray.length > 0, "Elements array is empty");
        require(traitArray.length == traitProbability.length, "Elements and weights length mismatch");
        
        for (uint i = 0; i < traitProbability.length; i++) {
            if(seed < traitProbability[i]) {
                return i;
            }
        }
        // Fallback, return first element as a safe default
        return 0;
    }

    function _reserveMoo() internal {
        uint startTokenID = _startTokenId() + _totalMinted();
        // Profile Moo
        TraitStruct memory profileTraits = TraitStruct({
            body: 0,
            spot: 0,
            mouth: 0,
            earring: 1,
            cig: 1,
            horn: 0,
            flying: 3,
            eye: 0,
            weather: 2
        });
        tokenTraits[startTokenID] = profileTraits;
        _safeMint(0x180CB989097F42A01Bb93c84E1f313015103146A, 1);
    }


    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        // Get image
        string memory image = buildSVG(tokenId);

        // Encode SVG data to base64
        string memory base64Image = Base64.encode(bytes(image));

        // Build JSON metadata
        string memory json = string(
            abi.encodePacked(
                '{"name": "OnChain FlyingCows #', Strings.toString(tokenId), '",',
                '"description": "Fully onchain flying cows going moo on base",',
                '"attributes": [', _getMooTraits(tokenId), '],',
                '"image": "data:image/svg+xml;base64,', base64Image, '"}'
            )
        );

        // Encode JSON data to base64
        string memory base64Json = Base64.encode(bytes(json));

        // Construct final URI
        return string(abi.encodePacked('data:application/json;base64,', base64Json));
    }

    function buildSVG(uint tokenid) public view returns (string memory) {

        require(_exists(tokenid), "Token does not exist");

        TraitStruct memory localTraits = tokenTraits[tokenid];

        string memory svg = string(abi.encodePacked(
        '<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" shape-rendering="crispEdges" width="512" height="512">',
            '<rect width="32" height="32" fill="#546d8e" />',
            _getSVGTraitData(body_data[localTraits.body]),
            _getSVGTraitData(spot_data[localTraits.spot]),
            _getSVGTraitData(mouth_data[localTraits.mouth]),
            _getSVGTraitData(earring_data[localTraits.earring]),
            _getSVGTraitData(cig_data[localTraits.cig]),
            _getSVGTraitData(horn_data[localTraits.horn]),
            _getSVGTraitData(flying_data[localTraits.flying]),
            _getSVGTraitData(eye_data[localTraits.eye]),
            _getSVGTraitData(weather_data[localTraits.weather]),
        '</svg>'
        ));
        return svg;

    }

    function _getSVGTraitData(bytes memory data) internal pure returns (string memory) {

        require(data.length % 7 == 0, "Invalid number of reacts");

        /// if empty this is a transparent react
        if (data.length == 0) {
             return "<rect x=\"0\" y=\"0\" width=\"0\" height=\"0\" fill=\"rgb(0,0,0)\"/>"; 
        }

        // Initialize arrays to store values
        uint reactCount = data.length / 7;


        /// react string to return
        string memory rects;

        // Iterate through each react and get the values we need
        for (uint i = 0; i < reactCount; i++) {

            // Convert values to strings
            string memory xStr = Strings.toString(uint8(data[i * 7]));
            string memory yStr = Strings.toString(uint8(data[i * 7 + 1]));
            string memory widthStr = Strings.toString(uint8(data[i * 7 + 2]));
            string memory heightStr = Strings.toString(uint8(data[i * 7 + 3]));
            string memory rStr = Strings.toString(uint8(data[i * 7 + 4]));
            string memory gStr = Strings.toString(uint8(data[i * 7 + 5]));
            string memory bStr = Strings.toString(uint8(data[i * 7 + 6]));

            rects = string(abi.encodePacked(rects, '<rect x="', xStr, '" y="', yStr, '" width="', widthStr, '" height="', heightStr, '" fill="rgb(', rStr, ',', gStr, ',', bStr, ')" />'));
        }

        return rects;
    }

    function _getMooTraits(uint tokenid) internal view returns (string memory) {

        TraitStruct memory traits = tokenTraits[tokenid];

        string memory metadata = string(abi.encodePacked(
        '{"trait_type":"Body", "value":"', body_traits[traits.body], '"},',
        '{"trait_type":"Spots", "value":"', spot_traits[traits.spot], '"},',
        '{"trait_type":"Mouth", "value":"', mouth_traits[traits.mouth], '"},',
        '{"trait_type":"Earring", "value":"', earring_traits[traits.earring], '"},',
        '{"trait_type":"Cig", "value":"', cig_traits[traits.cig], '"},',
        '{"trait_type":"Horn", "value":"', horn_traits[traits.horn], '"},',
        '{"trait_type":"Flying", "value":"', flying_traits[traits.flying], '"},',
        '{"trait_type":"Eye", "value":"', eye_traits[traits.eye], '"},',
        '{"trait_type":"Weather", "value":"', weather_traits[traits.weather], '"}'
        ));

        return metadata;

    }


//// Admin methods
    function toggleMinting() external onlyOwner {
        mintEnabled = !mintEnabled;
    }

    function setMaxFree(uint _newMaxFree) external onlyOwner {
        maxFree = _newMaxFree;
    }

    function devMint(uint _quantity) external onlyOwner {
        _internalMint(_quantity);
    }

    function addToWhiteList(address[] calldata addresses) external onlyOwner nonReentrant {
        for (uint i = 0; i < addresses.length; i++) {
            whiteListed[addresses[i]] = true;
        }
    }

    function withdraw() external onlyOwner nonReentrant {
        (bool success, ) = msg.sender.call{value: address(this).balance}("");
        require(success, "Transfer failed.");
    }

}